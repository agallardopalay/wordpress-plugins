{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/custom-video-lightbox",
  "version": "0.1.0",
  "title": " Video Lightbox",
  "category": "media",
  "icon": "embed-video",
  "description": "Hero section with video in a moda (scaffolded with Create Block tool).",
  "supports": {
    "html": false
  },
  "textdomain": "custom-video-lightbox",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "layout": {
      "enum": [
        "left",
        "right"
      ],
      "default": "left"
    },
    "videoType": {
      "enum": [
        "youtube",
        "vimeo"
      ],
      "default": "youtube"
    },
    "image": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src"
    },
    "downloadedMedia": {
      "type": "object"
    },
    "videoId": {
      "type": "string"
    },
    "subTitle": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    }
  }
}